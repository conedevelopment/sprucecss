# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
eclint_block_comment_start = /*
eclint_block_comment = *
eclint_block_comment_end = */

[*.{scss,css,js,json,yml,md,svg}]
indent_size = 2
